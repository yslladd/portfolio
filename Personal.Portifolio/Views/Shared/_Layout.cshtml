<!DOCTYPE html>
<html class="no-js">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=0" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1,requiresActiveX=true">

    <title> Daniel Melo :: Resumè </title>
    <!-- /// Favicons ////////  -->
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="apple-touch-icon-144-precomposed.png">
    <link rel="shortcut icon" href="favicon.png">

    <!-- /// Template CSS ////////  -->
    <link rel="stylesheet" href="@Url.Content("~/Content/css/base.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/css/grid.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/css/elements.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/css/layout.css")">
    <!-- /// JS Plugins CSS ////////  -->
    <link rel="stylesheet" href="@Url.Content("~/Scripts/bxslider/jquery.bxslider.css")">
    <link rel="stylesheet" href="@Url.Content("~/Scripts/magnificpopup/magnific-popup.css")">

    <!-- /// Skin CSS ////////  -->
    <!-- <link rel="stylesheet" href="_layout/css/skins/default.css">  -->
    <!-- /// Google Fonts ////////  -->
    <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=PT+Sans:400,700">
    <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Oswald:400,300,700">
    <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=PT+Sans+Narrow:400,700">

    <!-- /// FontAwesome Icons 4.0.3 ////////  -->
    <link rel="stylesheet" href="@Url.Content("~/Content/css/fontawesome/font-awesome.min.css")">

    <!-- /// Custom Icon Font ////////  -->
    <link rel="stylesheet" href="@Url.Content("~/Content/css/iconfontcustom/icon-font-custom.css")">

    <!-- /// Cross-browser CSS3 animations ////////  -->
    <link rel="stylesheet" href="@Url.Content("~/Content/css/animate/animate.min.css")">

    <!-- /// Modernizr ////////  -->
    <script src="@Url.Content("~/Scripts/modernizr-2.6.2.min.js")"></script>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>

    @RenderBody()

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")


    <!-- /// jQuery ////////  -->
    <script src="@Url.Content("~/Scripts/jquery-2.1.0.min.js")"></script>

    <!-- /// ViewPort ////////  -->
    <script src="@Url.Content("~/Scripts/viewport/jquery.viewport.js")"></script>

    <!-- /// Easing ////////  -->
    <script src="@Url.Content("~/Scripts/easing/jquery.easing.1.3.js")"></script>
    <!-- /// SimplePlaceholder ////////  -->
    <script src="@Url.Content("~/Scripts/simpleplaceholder/jquery.simpleplaceholder.js")"></script>
    <!-- /// Fitvids ////////  -->
    <script src="@Url.Content("~/Scripts/fitvids/jquery.fitvids.js")"></script>

    <!-- /// Superfish Menu ////////  -->
    <script src="@Url.Content("~/Scripts/superfish/hoverIntent.js")"></script>
    <script src="@Url.Content("~/Scripts/superfish/superfish.js")"></script>

    <!-- /// bxSlider ////////  -->
    <script src="@Url.Content("~/Scripts/bxslider/jquery.bxslider.min.js")"></script>

    <!-- /// Magnific Popup ////////  -->
    <script src="@Url.Content("~/Scripts/magnificpopup/jquery.magnific-popup.min.js")"></script>

    <!-- /// Isotope ////////  -->
    <script src="@Url.Content("~/Scripts/isotope/isotope.pkgd.min.js")"></script>
    <script src="@Url.Content("~/Scripts/isotope/imagesloaded.pkgd.min.js")"></script>

    <!-- /// Parallax ////////  -->
    <script src="@Url.Content("~/Scripts/parallax/jquery.parallax.min.js")"></script>

    <!-- /// EasyPieChart ////////  -->
    <script src="@Url.Content("~/Scripts/easypiechart/jquery.easypiechart.min.js")"></script>

    <!-- /// Easy Tabs ////////  -->
    <script src="@Url.Content("~/Scripts/easytabs/jquery.easytabs.min.js")"></script>

    <!-- /// Waypoints ////////  -->
    <script src="@Url.Content("~/Scripts/waypoints/waypoints.min.js")"></script>
    <script src="@Url.Content("~/Scripts/waypoints/waypoints-sticky.min.js")"></script>

    <!-- /// Form validate ////////  -->
    <script src="@Url.Content("~/Scripts/jqueryvalidate/jquery.validate.min.js")"></script>

    <!-- /// Form submit ////////  -->
    <script src="@Url.Content("~/Scripts/jqueryform/jquery.form.min.js")"></script>

    <!-- /// gMap ////////  -->
    <script src="http://maps.google.com/maps/api/js?sensor=false"></script>
    <script src="@Url.Content("~/Scripts/gmap/jquery.gmap.min.js")"></script>

    <!-- /// Scrollspy ////////  -->
    <script src="@Url.Content("~/Scripts/scrollspy/scrollspy.min.js")"></script>
    <!-- /// Custom JS ////////  -->
    <script src="@Url.Content("~/Scripts/custom.js")"></script>

    @RenderSection("scripts", required: false)

</body>
</html>
